{
  "trailingComma": "es5",
  "tabWidth": 4,
  "semi": false,
  "singleQuote": true,
  "overrides": [
    {
      "files": [
        "slides.md",
        "pages/*.md"
      ],
      "options": {
        "parser": "slidev",
        "plugins": [
          "prettier-plugin-slidev"
        ]
      }
    }
  ]
}